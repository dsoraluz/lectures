<h1><%= title %></h1>
<p>Welcome to <%= title %></p>

<ul>
  <% if (loggedUser) { %>
    Hello, <%= loggedUser.username %>!
    <li> <a href="/secret">Secret Logged-In Only Page</a> </li>
    <li> <a href="/logout">Log Out</a> </li>

  <% } else { %>

    <li> <a href="/signup">Sign Up</a> </li>
    <li> <a href="/login">Log In</a> </li>
  <% } %>
</ul>
